<wsdl:definitions
     name="MP3Writer"
     targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/file/FlightUpdateNotificationService_step2/FlightUpdateNotificationService/MP3Writer"
     xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/file/FlightUpdateNotificationService_step2/FlightUpdateNotificationService/MP3Writer"
     xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
